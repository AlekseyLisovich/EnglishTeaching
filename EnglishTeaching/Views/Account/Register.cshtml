@model EnglishTeaching.Models.Account.RegisterModel

@{
    ViewData["Title"] = "Sing Up";
}
<h2>Sing Up</h2>
<br />
<form asp-action="Register" asp-controller="Account" asp-anti-forgery="true">
    <div class="validation" asp-validation-summary="ModelOnly"></div>
    <div>
        <div class="form-group col-md-12">
            <label asp-for="Email" class="col-md-2 control-label">Enter Email</label>
            <div class="col-md-4">
                <input type="text" class="form-control" asp-for="Email" />
                <span asp-validation-for="Email" />
            </div>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="Password" class="col-md-2 control-label">Enter Email</label>
            <div class="col-md-4">
                <input class="form-control" asp-for="Password" />
                <span asp-validation-for="Password" />
            </div>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="ConfirmPassword" class="col-md-2 control-label">Confirm Password</label>
            <div class="col-md-4">
                <input class="form-control" asp-for="ConfirmPassword" />
                <span asp-validation-for="ConfirmPassword" />
            </div>
        </div>
        <div>
            <input type="submit" value="Sing Up" class="btn btn-success" />
        </div>
    </div>
</form>